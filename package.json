{
  "name": "dxw-playbook",
  "version": "1.0.0",
  "description": "dxw Playbook",
  "scripts": {
    "markdown:lint": "remark .",
    "vale": "~/go/bin/vale */*.md *.md",
    "vale-suggestions": "~/go/bin/vale --minAlertLevel suggestion */*.md *.md ",
    "build:dev": "webpack --mode=development",
    "build": "webpack -p --mode=production",
    "watch": "webpack --mode=development --watch",
    "js:lint": "eslint '_javascripts/src/**/*.js'",
    "js:lint:fix": "eslint '_javascripts/src/**/*.js' --fix",
    "prettier": "prettier --check '_javascripts/src/**/*.js' '_spec/*.js'",
    "prettier:fix": "prettier --write '_javascripts/src/**/*.js' '_spec/*.js'",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel": "^6.23.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "jest": "^26.0.1",
    "marked": "^1.0.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.9.0",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.0",
    "remark-preset-lint-markdown-style-guide": "^3.0.0",
    "template-polyfill": "^2.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "Event",
      "history"
    ]
  }
}
